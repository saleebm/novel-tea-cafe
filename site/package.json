    "develop": "gatsby develop",
    "develop:fast-refresh": "GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "npm run lint:ts && npm run stylelint",
    "lint:ts": "eslint './src/**/*' --ext .tsx,.ts",
    "lint:ts:fix": "eslint './src/**/*' --ext .tsx,.ts --fix",
    "pretty": "prettier --write --config .prettierrc '*/**/*.{js,jsx,ts,tsx}'",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "stylelint --fix \"src/**/*.scss\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
